{
  "name": "ag-grid-url-sync",
  "version": "0.1.1",
  "description": "AG Grid URL Sync - Synchronize AG Grid text filters with URL parameters",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf dist",
    "compile": "tsc",
    "build": "npm run clean && npm run compile && npm run validate",
    "validate": "npm run size-check && npm run type-check",
    "size-check": "bundlesize",
    "type-check": "tsc --noEmit --strict",
    "ci": "npm run build && npm run check-exports && npm run lint && npm run test",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nearform/ag-grid-url-state-sync.git"
  },
  "keywords": [
    "ag-grid",
    "url",
    "state",
    "sync",
    "filter",
    "share",
    "typescript"
  ],
  "author": "NearForm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nearform/ag-grid-url-state-sync/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/nearform/ag-grid-url-state-sync#readme",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@types/jsdom": "^21.1.7",
    "@vitest/coverage-v8": "^3.2.4",
    "ag-grid-community": "^33.3.2",
    "bundlesize": "^0.18.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "ag-grid-community": ">=28.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "bundlesize": [
    {
      "path": "./dist/index.js",
      "maxSize": "5KB"
    },
    {
      "path": "./dist/ag-grid-url-sync.js",
      "maxSize": "4KB"
    },
    {
      "path": "./dist/utils.js",
      "maxSize": "4KB"
    }
  ]
}
